@if (Model.Data != null)
{
    foreach (var listitem in Model.Data)
    {
        <li class="navlist">
            <div>
                <div>
                    <a href="@Sitecore.Links.LinkManager.GetItemUrl(listitem)">
                        @Html.Sitecore().Field("Icon", listitem, new { @class = "img-responsive" })
                    </a>
                </div>
                <div>
                    <a href="@Sitecore.Links.LinkManager.GetItemUrl(listitem)"><span class="nav-label">@listitem.Fields["Title"].Value</span> </a>
                </div>
            </div>
        </li>
    }
}
else
{
    <div class="row">
        <div class="col-lg-12">Please select a datasource</div>
    </div>
}
